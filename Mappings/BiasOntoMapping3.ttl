  a rr:TriplesMap;

  rml:logicalSource [
	rml:source "/Users/russo/git/SDM-RDFizer/mappings/input/datasetFolks.csv";
    rml:referenceFormulation ql:CSV
  ];

  rr:subjectMap [
  rr:template "https://nobias-project.eu/nobias/BiasMeasure/RepresentationRate-98";
